<?xml version="1.0" encoding="utf-8"?>

<com.gome.launcher.FolderIcon xmlns:android="http://schemas.android.com/apk/res/android"
                              android:id="@+id/move_item_folder"
                              android:layout_width="@dimen/move_item_width"
                              android:layout_height="match_parent">

    <ImageView
        android:id="@+id/preview_background"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:antialias="true"/>
    <com.gome.launcher.FolderIconPagedView
        android:id="@+id/folder_icon_content"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</com.gome.launcher.FolderIcon>

